Cell[TextData[{
 "Rata-rata dari data ",
 Cell[BoxData[
  FormBox["x", TraditionalForm]],ExpressionUUID->
  "a38af771-9b27-49b3-a29f-a40c61c2ebff"],
 " diberikan oleh ",
 Cell[BoxData[
  RowBox[{
   OverscriptBox["x", "_"], "=", 
   FractionBox[
    RowBox[{
     UnderoverscriptBox["\[Sum]", 
      RowBox[{"i", "=", "1"}], "n"], 
     SubscriptBox["x", "i"]}], "n"]}]],ExpressionUUID->
  "06ad22c6-c220-4d13-ab6f-35244d9782f5"],
 ", dengan kata lain, jumlah dari semua elemen di data ",
 Cell[BoxData[
  FormBox["x", TraditionalForm]],ExpressionUUID->
  "520364c4-a0db-4784-aafc-8843957260cb"],
 " dibagi dengan banyaknya data yaitu ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]],ExpressionUUID->
  "264cc41c-9ca3-4449-a29c-995a4be20220"],
 " dan\nStandar deviasi dari data ",
 Cell[BoxData[
  FormBox["x", TraditionalForm]],ExpressionUUID->
  "f3978bbe-1822-49ed-b433-2b5d0fda445d"],
 " diberikan oleh ",
 Cell[BoxData[
  RowBox[{"S", "=", 
   SqrtBox[
    FractionBox[
     RowBox[{
      UnderoverscriptBox["\[Sum]", 
       RowBox[{"i", "=", "1"}], "n"], 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{
         SubscriptBox["x", "i"], "-", 
         OverscriptBox["x", "_"]}], ")"}], "2"]}], 
     RowBox[{"n", "-", "1"}]]]}]],ExpressionUUID->
  "6862e874-1983-4914-98a9-51518b4d8bc4"],
 " karena ",
 Cell[BoxData[
  FormBox["x", TraditionalForm]],ExpressionUUID->
  "ea95fc23-6bfd-468b-8c35-7c0f42044615"],
 " adalah data sampel."
}], "Text",
