Cell[TextData[{
 "Jika ",
 Cell[BoxData[
  FormBox[
   RowBox[{"b", "=", "0"}], TraditionalForm]],ExpressionUUID->
  "892f7059-113c-4f6d-aebf-aab630a228f0"],
