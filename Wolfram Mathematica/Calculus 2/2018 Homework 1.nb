Cell[TextData[{
 "Kita juga dapat menggunakan fungsi built-in yaitu TransformedField. \
Spesifikasikan bahwa kita ingin mengubah dari kartesius menjadi polar. Ubah \
dahulu fungsi ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"f", "(", "x", ")"}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"-", "2"}], 
      SuperscriptBox["x", "2"]}], "+", "x", "+", "6"}]}], TraditionalForm]],
  ExpressionUUID->"3ca75a76-8672-4d2b-874f-1fb0287b8b5d"],
 " menjadi ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"f", "(", "x", ")"}], "+", 
     RowBox[{"2", 
      SuperscriptBox["x", "2"]}], "-", "x", "-", "6"}], "=", "0"}], 
   TraditionalForm]],ExpressionUUID->"09f0e9ab-20c1-47ee-bdb4-7ffe7ce4ebd2"],
 ". Misal ",
 Cell[BoxData[
  FormBox[
   RowBox[{"y", "=", 
    RowBox[{"f", "(", "x", ")"}]}], TraditionalForm]],ExpressionUUID->
  "266bd6fb-105d-413f-aa76-f5a49c331f62"],
 ". Sehingga:"
}], "Text",
 CellChangeTimes->{{3.7320824319187207`*^9, 
  3.732082511344672*^9}},ExpressionUUID->"705c24ae-fc66-4daa-912f-\
0b22a6a857ae"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"TransformedField", "[", 
  RowBox[{
   RowBox[{"\"\<Cartesian\>\"", "\[Rule]", "\"\<Polar\>\""}], ",", 
   RowBox[{"y", "+", 
    RowBox[{"2", 
     SuperscriptBox["x", "2"]}], "-", "x", "-", "6"}], ",", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"x", ",", "y"}], "}"}], "\[Rule]", 
    RowBox[{"{", 
     RowBox[{"r", ",", "\[Theta]"}], "}"}]}]}], "]"}]], "Input",ExpressionUUID\
->"99710023-bfb7-4529-b9da-7d864fbe9745"],

